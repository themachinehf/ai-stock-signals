{
  "buildCommand": "npm install",
  "outputDirectory": ".",
  "framework": null,
  "installCommand": "npm install",
  "regions": ["iad1"],
  "builds": [
    {
      "src": "api/handler.js",
      "use": "@vercel/node"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/handler.js"
    },
    {
      "source": "/(.*)",
      "destination": "/api/handler.js"
    }
  ]
}
