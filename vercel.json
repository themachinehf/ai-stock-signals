{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "framework": null,
  "installCommand": "pip install -r requirements.txt",
  "regions": ["iad1"],
  "builds": [
    {
      "src": "api/handler.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12",
        "enableServerlessSourcemaps": false
      }
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/handler.py"
    },
    {
      "source": "/(.*)",
      "destination": "/api/handler.py"
    }
  ],
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
