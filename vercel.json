{
  "buildCommand": "pip install -r requirements.txt",
  "outputDirectory": ".",
  "framework": null,
  "installCommand": "pip install -r requirements.txt",
  "regions": ["iad1"],
  "builds": [
    {
      "src": "api/handler.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.12"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/handler.py"
    },
    {
      "src": "/(.*)",
      "dest": "/api/handler.py"
    }
  ]
}
